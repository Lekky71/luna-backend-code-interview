{
  "name": "nft-metadata-api",
  "version": "1.0.0",
  "description": "An ERC721 NFT Metadata API",
  "main": "./src/bin/www/index.ts",
  "private": true,
  "scripts": {
    "dev": "npm run migrate && ts-node ./src/bin/www/index.ts",
    "prod:start": "NODE_ENV=production node ./dist/bin/www/index.js",
    "dev:watch": "nodemon ./src/bin/www/index.ts",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watchAll --collectCoverage=false",
    "test:vulnerability": "npm audit",
    "lint": "eslint './src/**/*.ts' --max-warnings 0",
    "clean": "rm -rf coverage dist",
    "migrate": "knex migrate:latest",
    "unmigrate": "knex migrate:rollback",
    "seed": "knex seed:run "
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lekky71/luna-backend-code-interview.git"
  },
  "keywords": [
    "NodeJs API",
    "ExpressJs",
    "Typescript"
  ],
  "author": {
    "name": "Lekky71",
    "email": "hashilekky@gmail.com"
  },
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/Lekky71/luna-backend-code-interview.git/issues"
  },
  "homepage": "https://github.com/Lekky71/luna-backend-code-interview.git#readme",
  "dependencies": {
    "body-parser": "1.19.2",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "errorhandler": "1.5.1",
    "express": "4.17.3",
    "express-validator": "6.14.0",
    "helmet": "5.0.2",
    "knex": "^1.0.3",
    "log4js": "^6.4.2",
    "method-override": "3.0.0",
    "pg": "8.7.3"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/errorhandler": "1.5.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.4.1",
    "@types/method-override": "0.0.32",
    "@types/node": "17.0.21",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "5.14.0",
    "@typescript-eslint/parser": "5.14.0",
    "eslint": "8.10.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "supertest": "4.0.2",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.7",
    "ts-node": "10.7.0",
    "typescript": "4.6.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-node-externals": "3.0.0"
  }
}
